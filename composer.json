{
    "name": "towa/gebrueder-weiss-woocommerce",
    "description": "Gebr√ºder Weiss Woocommerce Plugin",
    "type": "wordpress-plugin",
    "require": {
        "guzzlehttp/guzzle": "*",
        "towa/gbw-sdk": "^1.0"
    },
    "homepage": "https://www.towa-digital.com",
    "require-dev": {
        "php": "^7.3|^8.0",
        "phpunit/phpunit": "^7",
        "squizlabs/php_codesniffer": "^3.6",
        "wp-coding-standards/wpcs": "^2.3"
    },
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Martin Welte",
            "email": "martin.welte@towa.at"
        }
    ],
    "support": {
      "issues": "https://github.com/towa-digital/gebrueder-weiss-woocommerce/issues",
      "source": "https://github.com/towa-digital/gebrueder-weiss-woocommerce",
      "docs": "https://timber.github.io/docs/"
    },
    "minimum-stability": "stable",
    "autoload": {
      "psr-4": {
        "GbWeiss\\lib\\": "lib/",
        "GbWeiss\\includes\\": "includes/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\" :"tests/"
      }
    },
    "scripts": {
      "fix-cs": "phpcbf --standard=PSR12 ./includes/",
      "lint": "phpcs --standard=PSR12 ./includes/"
    }
}